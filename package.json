{
  "name": "cssgraph",
  "description": "use css(scss) to graph",
  "main": "index.js",
  "scripts": {
    "start": "node copy.js",
    "wp": "node webpack.server.js",
    "dev": "lite-server",
    "build": "npm run watch:pug & npm run watch:stylus & npm run watch:js",
    "prebuild:pug": "rm -rf dist/html/*",
    "build:pug": "mkdir -p dist/html && pug pug/*.pug --pretty -o dist/html",
    "postbuild:pug": "rm -rf dist/html/common.html dist/html/header.html",
    "precompile:js": "rm -rf dist/js/*",
    "compile:js": "mkdir -p dist/js/temp && babel src/scripts/index.js --out-dir dist/js/temp/",
    "prebuild:js": "rm -rf dist/js/*",
    "build:js": "mkdir -p dist/js/temp && uglifyjs src/scripts/*.js -m -o dist/js/temp/app.js",
    "postbuild:js": "hashmark -s -l 16 -d md5 dist/js/temp/*.js dist/js/{name}.{hash}{ext} && rm -rf dist/js/temp",
    "prebuild:stylus": "rm -rf dist/css/*",
    "build:stylus": "mkdir -p dist/css/temp && stylus css/test.styl -r -m -c --include-css -o dist/css/temp/app.css",
    "postbuild:stylus": "hashmark -s -l 16 -d md5 dist/css/temp/*.{css,map} dist/css/{name}.{hash}{ext} --asset-map dist/css/assets.json && rm -rf dist/css/temp",
    "watch:pug": "nodemon -w pug -e pug,jade -x 'npm run build:pug'",
    "watch:js": "nodemon -w src -e js,jsx -x 'npm run build:js'",
    "watch:scss": "nodemon -w css -e scss,sass -x 'npm run build:scss'",
    "watch:stylus": "nodemon -w css -e stylus,styl -x 'npm run build:stylus'",
    "env": "env",
    "test": "jest"
  },
  "repository": {
    "url": "https://github.com/xxxxxMiss",
    "type": "git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node git-hooks-commitmsg.js",
      "pre-commit": "node git-hooks-precommitmsg.js"
    }
  },
  "author": "qsch",
  "license": "MIT",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "chai": "^4.1.2",
    "cowsay": "^1.1.9",
    "raw-loader": "^3.1.0"
  },
  "dependencies": {
    "@babel/generator": "^7.10.4",
    "@babel/parser": "^7.10.4",
    "@babel/traverse": "^7.10.4",
    "@babel/types": "^7.10.5",
    "@zeit/next-sass": "^1.0.1",
    "execa": "^0.8.0",
    "express": "^4.15.4",
    "finalhandler": "^1.1.1",
    "handlebars": "^4.0.11",
    "husky": "^1.3.1",
    "ic-persistentjs": "^1.0.1",
    "immutable": "^3.8.2",
    "inquirer": "^3.3.0",
    "internal-ip": "^3.0.1",
    "isomorphic-git": "^0.51.4",
    "joi-browser": "^13.4.0",
    "lodash": "^4.17.10",
    "memfs": "^2.14.2",
    "mongoose": "^4.13.2",
    "nanoid": "^1.3.1",
    "normalizr": "^3.2.4",
    "on-finished": "^2.3.0",
    "open": "0.0.5",
    "ora": "^1.3.0",
    "path-to-regexp": "^2.2.1",
    "public-ip": "^3.0.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "semver": "^5.4.1",
    "send": "^0.16.2",
    "serve-static": "^1.13.2",
    "tapable": "^1.1.1",
    "yargs": "^15.4.1"
  }
}
